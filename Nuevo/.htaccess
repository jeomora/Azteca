<IfModule mod_rewrite.c>

    RewriteEngine On

    RewriteBase /

    RewriteCond %{REQUEST_FILENAME} !-f

    RewriteCond %{REQUEST_FILENAME} !-d

    RewriteRule ^(.*)$ Aztecas/Nuevo/index.php?/$1 [L]

</IfModule>

<IfModule !mod_rewrite.c>

    ErrorDocument 404 /Aztecas/Nuevo/index.php

</IfModule>

